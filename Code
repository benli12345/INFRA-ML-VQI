#Train and evaluate XGBoost for predicting secondary outcomes

XGB_thromb_preop <- xgboost(data = as.matrix(predictors_train_preop), label = as.numeric(train$thromb), nrounds = 150, verbose = TRUE, objective = 'binary:logistic', eval_metric = 'auc', max_depth = 4, eta = 0.3, gamma = 0, colsample_bytree = 0.6, min_child_weight = 3, subsample = 0.9) #hyperparameters chosen based on above grid search and cross validation
XGB_thromb_pred_preop <- predict(XGB_thromb_preop, as.matrix(predictors_test_preop))
auc(test$thromb, XGB_thromb_pred_preop)
ci.auc(test$thromb, XGB_thromb_pred_preop)
XGB_thromb_pred_preop_0.5 <- ifelse(XGB_thromb_pred_preop > 0.5,1,0)
confusionMatrix(as.factor(test$thromb), as.factor(XGB_thromb_pred_preop_0.5))

XGB_thromb_intraop <- xgboost(data = as.matrix(predictors_train_intraop), label = as.numeric(train$thromb), nrounds = 150, verbose = TRUE, objective = 'binary:logistic', eval_metric = 'auc', max_depth = 4, eta = 0.3, gamma = 0, colsample_bytree = 0.6, min_child_weight = 3, subsample = 0.9)
XGB_thromb_pred_intraop <- predict(XGB_thromb_intraop, as.matrix(predictors_test_intraop))
auc(test$thromb, XGB_thromb_pred_intraop)
ci.auc(test$thromb, XGB_thromb_pred_intraop)
XGB_thromb_pred_intraop_0.5 <- ifelse(XGB_thromb_pred_intraop > 0.5,1,0)
confusionMatrix(as.factor(test$thromb), as.factor(XGB_thromb_pred_intraop_0.5))

XGB_thromb_postop <- xgboost(data = as.matrix(predictors_train_postop), label = as.numeric(train$thromb), nrounds = 150, verbose = TRUE, objective = 'binary:logistic', eval_metric = 'auc', max_depth = 4, eta = 0.3, gamma = 0, colsample_bytree = 0.6, min_child_weight = 3, subsample = 0.9)
XGB_thromb_pred_postop <- predict(XGB_thromb_postop, as.matrix(predictors_test_postop))
auc(test$thromb, XGB_thromb_pred_postop)
ci.auc(test$thromb, XGB_thromb_pred_postop)
XGB_thromb_pred_postop_0.5 <- ifelse(XGB_thromb_pred_postop > 0.5,1,0)
confusionMatrix(as.factor(test$thromb), as.factor(XGB_thromb_pred_postop_0.5))

XGB_surg_preop <- xgboost(data = as.matrix(predictors_train_preop), label = as.numeric(train$surg), nrounds = 150, verbose = TRUE, objective = 'binary:logistic', eval_metric = 'auc', max_depth = 4, eta = 0.3, gamma = 0, colsample_bytree = 0.6, min_child_weight = 3, subsample = 0.9)
XGB_surg_pred_preop <- predict(XGB_surg_preop, as.matrix(predictors_test_preop))
auc(test$surg, XGB_surg_pred_preop)
ci.auc(test$surg, XGB_surg_pred_preop)
XGB_surg_pred_preop_0.5 <- ifelse(XGB_surg_pred_preop > 0.5,1,0)
confusionMatrix(as.factor(test$surg), as.factor(XGB_surg_pred_preop_0.5))

XGB_surg_intraop <- xgboost(data = as.matrix(predictors_train_intraop), label = as.numeric(train$surg), nrounds = 150, verbose = TRUE, objective = 'binary:logistic', eval_metric = 'auc', max_depth = 4, eta = 0.3, gamma = 0, colsample_bytree = 0.6, min_child_weight = 3, subsample = 0.9)
XGB_surg_pred_intraop <- predict(XGB_surg_intraop, as.matrix(predictors_test_intraop))
auc(test$surg, XGB_surg_pred_intraop)
ci.auc(test$surg, XGB_surg_pred_intraop)
XGB_surg_pred_intraop_0.5 <- ifelse(XGB_surg_pred_intraop > 0.5,1,0)
confusionMatrix(as.factor(test$surg), as.factor(XGB_surg_pred_intraop_0.5))

XGB_surg_postop <- xgboost(data = as.matrix(predictors_train_postop), label = as.numeric(train$surg), nrounds = 150, verbose = TRUE, objective = 'binary:logistic', eval_metric = 'auc', max_depth = 4, eta = 0.3, gamma = 0, colsample_bytree = 0.6, min_child_weight = 3, subsample = 0.9)
XGB_surg_pred_postop <- predict(XGB_surg_postop, as.matrix(predictors_test_postop))
auc(test$surg, XGB_surg_pred_postop)
ci.auc(test$surg, XGB_surg_pred_postop)
XGB_surg_pred_postop_0.5 <- ifelse(XGB_surg_pred_postop > 0.5,1,0)
confusionMatrix(as.factor(test$surg), as.factor(XGB_surg_pred_postop_0.5))

XGB_amp_preop <- xgboost(data = as.matrix(predictors_train_preop), label = as.numeric(train$amp), nrounds = 150, verbose = TRUE, objective = 'binary:logistic', eval_metric = 'auc', max_depth = 4, eta = 0.3, gamma = 0, colsample_bytree = 0.6, min_child_weight = 3, subsample = 0.9)
XGB_amp_pred_preop <- predict(XGB_amp_preop, as.matrix(predictors_test_preop))
auc(test$amp, XGB_amp_pred_preop)
ci.auc(test$amp, XGB_amp_pred_preop)
XGB_amp_pred_preop_0.5 <- ifelse(XGB_amp_pred_preop > 0.5,1,0)
confusionMatrix(as.factor(test$amp), as.factor(XGB_amp_pred_preop_0.5))

XGB_amp_intraop <- xgboost(data = as.matrix(predictors_train_intraop), label = as.numeric(train$amp), nrounds = 150, verbose = TRUE, objective = 'binary:logistic', eval_metric = 'auc', max_depth = 4, eta = 0.3, gamma = 0, colsample_bytree = 0.6, min_child_weight = 3, subsample = 0.9)
XGB_amp_pred_intraop <- predict(XGB_amp_intraop, as.matrix(predictors_test_intraop))
auc(test$amp, XGB_amp_pred_intraop)
ci.auc(test$amp, XGB_amp_pred_intraop)
XGB_amp_pred_intraop_0.5 <- ifelse(XGB_amp_pred_intraop > 0.5,1,0)
confusionMatrix(as.factor(test$amp), as.factor(XGB_amp_pred_intraop_0.5))

XGB_amp_postop <- xgboost(data = as.matrix(predictors_train_postop), label = as.numeric(train$amp), nrounds = 150, verbose = TRUE, objective = 'binary:logistic', eval_metric = 'auc', max_depth = 4, eta = 0.3, gamma = 0, colsample_bytree = 0.6, min_child_weight = 3, subsample = 0.9)
XGB_amp_pred_postop <- predict(XGB_amp_postop, as.matrix(predictors_test_postop))
auc(test$amp, XGB_amp_pred_postop)
ci.auc(test$amp, XGB_amp_pred_postop)
XGB_amp_pred_postop_0.5 <- ifelse(XGB_amp_pred_postop > 0.5,1,0)
confusionMatrix(as.factor(test$amp), as.factor(XGB_amp_pred_postop_0.5))

XGB_death_preop <- xgboost(data = as.matrix(predictors_train_preop), label = as.numeric(train$death), nrounds = 150, verbose = TRUE, objective = 'binary:logistic', eval_metric = 'auc', max_depth = 4, eta = 0.3, gamma = 0, colsample_bytree = 0.6, min_child_weight = 3, subsample = 0.9)
